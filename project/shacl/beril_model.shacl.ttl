@prefix beril_model: <https://w3id.org/beril-model/beril-model/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema1: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schema1:Thing a sh:NodeShape ;
    sh:closed true ;
    sh:description "A generic grouping for any identifiable entity" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema1:description ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path beril_model:type_value ;
            sh:pattern "^([a-zA-Z_][a-zA-Z0-9_-]*):([a-zA-Z_][a-zA-Z0-9_-]*)$" ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema1:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema1:identifier ;
            sh:pattern "[a-zA-Z0-9_]+:[a-zA-Z0-9_]+" ] ;
    sh:targetClass schema1:Thing .

beril_model:DnaExtract a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path schema1:identifier ;
            sh:pattern "[a-zA-Z0-9_]+:[a-zA-Z0-9_]+" ],
        [ sh:in ( "RED" "GREEN" "BLUE" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path beril_model:color ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path beril_model:type_value ;
            sh:pattern "^([a-zA-Z_][a-zA-Z0-9_-]*):([a-zA-Z_][a-zA-Z0-9_-]*)$" ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema1:description ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path beril_model:mass_g ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema1:name ] ;
    sh:targetClass beril_model:DnaExtract .

beril_model:InformationArtifact a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The MD5 checksum of a file" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path beril_model:md5 ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path schema1:identifier ;
            sh:pattern "[a-zA-Z0-9_]+:[a-zA-Z0-9_]+" ],
        [ sh:description "The size of a file in bytes" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path beril_model:size_in_bytes ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema1:name ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path schema1:description ],
        [ sh:description "A URL for a file" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path beril_model:url ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 6 ;
            sh:path beril_model:type_value ;
            sh:pattern "^([a-zA-Z_][a-zA-Z0-9_-]*):([a-zA-Z_][a-zA-Z0-9_-]*)$" ] ;
    sh:targetClass beril_model:InformationArtifact .

beril_model:NamedThingCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "A multi-valued, inlined-as-list grouping of instances from any class, provided a suitable linking slot is provided" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class beril_model:Process ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path beril_model:processes ],
        [ sh:class beril_model:MaterialEntity ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path beril_model:material_entities ] ;
    sh:targetClass beril_model:NamedThingCollection .

beril_model:NonProcess a sh:NodeShape ;
    sh:closed true ;
    sh:description "A grouping for any named thing that is not a process" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema1:identifier ;
            sh:pattern "[a-zA-Z0-9_]+:[a-zA-Z0-9_]+" ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema1:description ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path beril_model:type_value ;
            sh:pattern "^([a-zA-Z_][a-zA-Z0-9_-]*):([a-zA-Z_][a-zA-Z0-9_-]*)$" ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema1:name ] ;
    sh:targetClass beril_model:NonProcess .

beril_model:Observation a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The raw value of an observation" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path beril_model:raw_value ] ;
    sh:targetClass beril_model:Observation .

beril_model:SoilSample a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path schema1:identifier ;
            sh:pattern "[a-zA-Z0-9_]+:[a-zA-Z0-9_]+" ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema1:description ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path beril_model:type_value ;
            sh:pattern "^([a-zA-Z_][a-zA-Z0-9_-]*):([a-zA-Z_][a-zA-Z0-9_-]*)$" ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path beril_model:mass_g ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema1:name ],
        [ sh:in ( "RED" "GREEN" "BLUE" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path beril_model:color ] ;
    sh:targetClass beril_model:SoilSample .

beril_model:Process a sh:NodeShape ;
    sh:closed true ;
    sh:description "Something that unfolds over time" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema1:description ],
        [ sh:class beril_model:MaterialEntity ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path beril_model:inputs ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path schema1:identifier ;
            sh:pattern "[a-zA-Z0-9_]+:[a-zA-Z0-9_]+" ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema1:name ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path beril_model:type_value ;
            sh:pattern "^([a-zA-Z_][a-zA-Z0-9_-]*):([a-zA-Z_][a-zA-Z0-9_-]*)$" ],
        [ sh:class beril_model:MaterialEntity ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path beril_model:outputs ] ;
    sh:targetClass beril_model:Process .

beril_model:MaterialEntity a sh:NodeShape ;
    sh:closed true ;
    sh:description "something that is made of matter" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema1:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path schema1:identifier ;
            sh:pattern "[a-zA-Z0-9_]+:[a-zA-Z0-9_]+" ],
        [ sh:in ( "RED" "GREEN" "BLUE" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path beril_model:color ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path beril_model:type_value ;
            sh:pattern "^([a-zA-Z_][a-zA-Z0-9_-]*):([a-zA-Z_][a-zA-Z0-9_-]*)$" ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path beril_model:mass_g ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema1:description ] ;
    sh:targetClass beril_model:MaterialEntity .

