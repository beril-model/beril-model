      "name": "md5",
      "definition_uri": "https://w3id.org/beril-model/beril-model/md5",
      "description": "The MD5 checksum of a file",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "slot_uri": "https://w3id.org/beril-model/beril-model/md5",
      "owner": "InformationArtifact",
      "domain_of": [
        "InformationArtifact"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "url",
      "definition_uri": "https://w3id.org/beril-model/beril-model/url",
      "description": "A URL for a file",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "slot_uri": "https://w3id.org/beril-model/beril-model/url",
      "owner": "InformationArtifact",
      "domain_of": [
        "InformationArtifact"
      ],
      "range": "uriorcurie",
      "@type": "SlotDefinition"
    },
    {
      "name": "inputs",
      "definition_uri": "https://w3id.org/beril-model/beril-model/inputs",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "slot_uri": "https://w3id.org/beril-model/beril-model/inputs",
      "multivalued": true,
      "owner": "Process",
      "domain_of": [
        "Process"
      ],
      "range": "MaterialEntity",
      "@type": "SlotDefinition"
    },
    {
      "name": "outputs",
      "definition_uri": "https://w3id.org/beril-model/beril-model/outputs",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "slot_uri": "https://w3id.org/beril-model/beril-model/outputs",
      "multivalued": true,
      "owner": "Process",
      "domain_of": [
        "Process"
      ],
      "range": "MaterialEntity",
      "@type": "SlotDefinition"
    },
