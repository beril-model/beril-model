      "owner": "PersonCollection",
      "domain_of": [
        "PersonCollection"
      ],
      "range": "Person",
      "inlined": true,
      "inlined_as_list": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "Person_primary_email",
      "definition_uri": "https://w3id.org/beril-model/beril-model/primary_email",
      "description": "The main email address of a person",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "mappings": [
        "http://schema.org/email"
      ],
      "is_a": "primary_email",
      "domain": "Person",
      "slot_uri": "http://schema.org/email",
      "alias": "primary_email",
      "owner": "Person",
      "domain_of": [
        "Person"
      ],
      "is_usage_slot": true,
      "usage_slot_name": "primary_email",
      "range": "string",
      "pattern": "^\\S+@[\\S+\\.]+\\S+",
      "@type": "SlotDefinition"
    }
  ],
  "classes": [
    {
      "name": "NamedThing",
      "definition_uri": "https://w3id.org/beril-model/beril-model/NamedThing",
      "description": "A generic grouping for any identifiable entity",
      "notes": [
        "What kinds of things wouldn't be named? Observations that only have relations with types, not instances?"
      ],
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "mappings": [
        "schema:Thing"
      ],
      "slots": [
        "id",
        "name",
        "description"
      ],
      "slot_usage": {},
      "class_uri": "http://schema.org/Thing",
      "@type": "ClassDefinition"
    },
    {
      "name": "Observation",
      "definition_uri": "https://w3id.org/beril-model/beril-model/Observation",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "slots": [
        "raw_value"
      ],
      "slot_usage": {},
      "class_uri": "https://w3id.org/beril-model/beril-model/Observation",
      "disjoint_with": [
        "NamedThing"
      ],
      "@type": "ClassDefinition"
    },
    {
      "name": "NonProcess",
      "definition_uri": "https://w3id.org/beril-model/beril-model/NonProcess",
      "description": "A grouping for any named thing that is not a process",
      "notes": [
        "At the very least we want the ability to say that Processes can have either a MaterialEntity or a DataArtifact as its input and output. The LinkML AnyOf range construct needs maintenance"
      ],
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "is_a": "NamedThing",
      "slots": [
        "id",
        "name",
        "description"
      ],
      "slot_usage": {},
      "class_uri": "https://w3id.org/beril-model/beril-model/NonProcess",
      "@type": "ClassDefinition"
    },
    {
      "name": "MaterialEntity",
      "definition_uri": "https://w3id.org/beril-model/beril-model/MaterialEntity",
      "description": "something that is made of matter",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "is_a": "NonProcess",
      "slots": [
        "id",
        "name",
        "description",
        "observations"
      ],
      "slot_usage": {},
      "class_uri": "https://w3id.org/beril-model/beril-model/MaterialEntity",
      "disjoint_with": [
        "InformationArtifact",
        "Process",
        "Observation"
      ],
      "@type": "ClassDefinition"
    },
    {
      "name": "InformationArtifact",
      "definition_uri": "https://w3id.org/beril-model/beril-model/InformationArtifact",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "is_a": "NonProcess",
      "slots": [
        "id",
        "name",
        "description",
        "size_in_bytes",
        "md5",
        "url"
      ],
      "slot_usage": {},
      "class_uri": "https://w3id.org/beril-model/beril-model/InformationArtifact",
      "disjoint_with": [
        "MaterialEntity",
        "Process",
        "Observation"
      ],
      "@type": "ClassDefinition"
    },
    {
      "name": "Process",
      "definition_uri": "https://w3id.org/beril-model/beril-model/Process",
      "description": "Something that unfolds over time",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "is_a": "NamedThing",
      "slots": [
        "id",
        "name",
        "description",
        "inputs",
        "outputs"
      ],
      "slot_usage": {},
      "class_uri": "https://w3id.org/beril-model/beril-model/Process",
      "disjoint_with": [
        "MaterialEntity",
        "InformationArtifact",
        "Observation"
      ],
      "@type": "ClassDefinition"
    },
    {
      "name": "Person",
      "definition_uri": "https://w3id.org/beril-model/beril-model/Person",
      "description": "Represents a Person",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "is_a": "NamedThing",
      "slots": [
        "id",
        "name",
        "description",
        "Person_primary_email",
        "birth_date",
        "age_in_years",
        "vital_status"
      ],
      "slot_usage": {},
      "class_uri": "https://w3id.org/beril-model/beril-model/Person",
      "@type": "ClassDefinition"
    },
    {
      "name": "PersonCollection",
      "definition_uri": "https://w3id.org/beril-model/beril-model/PersonCollection",
      "description": "A holder for Person objects",
      "from_schema": "https://w3id.org/beril-model/beril-model",
      "slots": [
        "personCollection__entries"
      ],
      "slot_usage": {},
      "attributes": [
        {
          "name": "entries",
          "multivalued": true,
          "range": "Person",
          "inlined": true,
