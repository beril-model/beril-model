      "from_schema": "https://w3id.org/beril-model/beril-model",
      "slot_uri": "https://w3id.org/beril-model/beril-model/vital_status",
      "owner": "Person",
      "domain_of": [
        "Person"
      ],
      "range": "PersonStatus",
      "@type": "SlotDefinition"
    },
    {
      "name": "raw_value",
